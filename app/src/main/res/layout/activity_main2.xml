<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CCC6C6"
    tools:context=".MainActivity2">

    <View
        android:id="@+id/view2"
        android:layout_width="354dp"
        android:layout_height="544dp"
        android:background="#A6A3A3"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="99dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="تكنولوجيا المعلومات في مكان العمل"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textDirection="firstStrongRtl"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="127dp" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="الوصف"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="مدة الدورة:"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="549dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ثماني ساعات فما فوق"
        android:textAlignment="viewStart"
        android:textColor="#423F3F"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="549dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="عن بُعد"
        android:textAlignment="viewStart"
        android:textColor="#423F3F"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="503dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="الشروط:"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="590dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="نوع النشاط:"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="503dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="#CDC9C9"
        android:tint="#434242"
        app:srcCompat="@android:drawable/ic_dialog_dialer"
        tools:layout_editor_absoluteX="349dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/back2"
        android:layout_width="53dp"
        android:layout_height="42dp"
        android:background="#CDC9C9"
        android:layoutDirection="ltr"
        android:tint="#000000"
        app:srcCompat="?attr/actionModeCloseDrawable"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="90dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/twaq"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="7dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="332dp"
        android:layout_height="240dp"
        app:srcCompat="@drawable/it"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/link2"
        android:textAlignment="viewStart"
        android:textColor="#423F3F"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="591dp" />

</androidx.constraintlayout.widget.ConstraintLayout>